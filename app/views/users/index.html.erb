<turbo-frame id="main-content">
  <div class="subHeader">
    <h2> MySubHeader</h2>
  </div>
  <p style="color: green"><%= notice %></p>

  <h1>Users</h1>

  <div id="users">
    <% @users.each do |user| %>
      <div id="<%= dom_id user %>">
        <p>
          <strong>Name:</strong>
          <%= user.name %>
        </p>
      </div>
      <p>
        <%= link_to "Show this user", user, 'data-turbo-frame': "main-content", 'data-turbo-action': "advance" %>
      </p>
    <% end %>
  </div>

  <%= link_to "New user", new_user_path %>
</turbo-frame>
